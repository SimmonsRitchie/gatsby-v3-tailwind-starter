{
  "name": "gatsby-v3-tailwind-starter",
  "description": "Gatsby v3 starter template, styled with Tailwind CSS",
  "version": "1.0.0",
  "author": "Daniel Simmons-Ritchie",
  "dependencies": {
    "gatsby": "^3.2.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-postcss": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sass": "^4.2.0",
    "gatsby-plugin-sharp": "^3.7.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "typeface-montserrat": "^1.1.13",
    "typeface-open-sans": "^1.1.13"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cleandev": "npm run clean && npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "10.1.0",
    "cssnano": "^5.0.6",
    "eslint": "^7.29.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "gatsby-plugin-eslint": "3.0.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "sass": "^1.32.8",
    "tailwindcss": "^2.2.2"
  }
}
